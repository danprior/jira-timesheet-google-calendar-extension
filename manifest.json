{
  "manifest_version": 2,
  "name": "Google Calendar Timesheets",
  "description": "Log time in JIRA based on your Google calendar.",
  "version": "1.4.3",
  "permissions": [
    "identity", "storage", "tabs", "https://*.atlassian.net/secure/Tempo*", "https://*.googleapis.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://*.atlassian.net/secure/Tempo*"],
      "js": ["jquery-2.1.1.min.js","chrome_ext.js"]
    }
  ],
  "icons": {
    "16": "icon-128.png",
    "48": "icon-128.png",
    "128": "icon-128.png"
  },
  "key": "",
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://apis.google.com; object-src 'self'",
  "permissions": [
    "storage",
    "activeTab",
    "identity",
    "https://ajax.googleapis.com/"
  ],
  "options_page": "options.html",
  "background": {
    "scripts": ["eventPage.js"],
    "persistent": true
  }
}
